{
  "rewrites": [
    { "source": "/login", "destination": "https://covex-dashboard-clean-one.vercel.app/login" },
    { "source": "/login/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/login/:path*" },

    { "source": "/dashboard", "destination": "https://covex-dashboard-clean-one.vercel.app/dashboard" },
    { "source": "/appointments/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/appointments/:path*" },
    { "source": "/calendar/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/calendar/:path*" },
    { "source": "/services/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/services/:path*" },
    { "source": "/settings/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/settings/:path*" },

    { "source": "/_next/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/_next/:path*" },
    { "source": "/api/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/api/:path*" },
    { "source": "/favicon.ico", "destination": "https://covex-dashboard-clean-one.vercel.app/favicon.ico" },
    { "source": "/assets/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/assets/:path*" },
    { "source": "/fonts/:path*", "destination": "https://covex-dashboard-clean-one.vercel.app/fonts/:path*" }
